<script>
 $(document).ready(function() {
    // Function to handle showing the correct product variants
    function handleSelection($bigSelect) {
        const $selectElement = $bigSelect.find('select');
        const $variantDivs = $bigSelect.find('.product__variants');
 const $variantSelects = $bigSelect.find('.side-selects');
        // Function to show the correct variant and hide others
        function showVariant() {
            const selectedIndex = $selectElement.prop('selectedIndex');
            $variantDivs.hide().eq(selectedIndex).show();
            $variantSelects.hide().eq(selectedIndex).show();
        }

        // Initial display setup
        showVariant();

        // Event listener for select change
        $selectElement.on('change', showVariant);
    }

    // Apply the handleSelection function to each .big-select container
    $('.big-select').each(function() {
        handleSelection($(this));
    });
});

